package entra21.aplicacao;

import entra21.janela.MenuPrincipal;
import entra21.validacao.PreencheVetores;

public class Inicializa {
	public static void main(String[] args) {
		PreencheVetores preenche = new PreencheVetores();
		preenche.vetorNomeFuncionario();
		preenche.vetorIdadeFuncionario();
		preenche.vetorCPFFuncionario();
		
		MenuPrincipal menuPrincipal = new MenuPrincipal();
		menuPrincipal.monstraMenu();
	}
}
